{
  "number": 55,
  "title": "Santo! Santo! Santo!",
  "marking": "Majestosamente",
  "verses": [
    {
      "number": 1,
      "lines": [
        "Santo! Santo! Santo!",
        "Deus onipotente!",
        "Cantam, de manh\u00e3, nossas vozes com ardor.",
        "Santo! Santo! Santo!",
        "Justo e compassivo!",
        "\u00c9s nosso Deus, excelso Criador!"
      ]
    },
    {
      "number": 2,
      "lines": [
        "Santo! Santo! Santo!",
        "Todos os remidos,",
        "Juntos com os anjos, proclamam teu louvor.",
        "Antes de formar-se o",
        "firmamento e a terra",
        "Eras e sempre \u00e9s, e h\u00e1s de ser, Senhor!"
      ]
    },
    {
      "number": 3,
      "lines": [
        "Santo! Santo! Santo!",
        "N\u00f3s, os pecadores,",
        "N\u00e3o podemos ver tua gl\u00f3ria sem tremor.",
        "Santo! Santo! Santo!",
        "S\u00f3 tu \u00e9s perfeito.",
        "Deus soberano, imenso em teu amor!"
      ]
    },
    {
      "number": 4,
      "lines": [
        "Santo! Santo! Santo!",
        "Deus onipotente!",
        "Tuas obras louvam teu nome com fervor.",
        "Santo! Santo! Santo!",
        "Justo e compassivo!",
        "\u00c9s nosso Deus, Excelso Criador!"
      ]
    }
  ],
