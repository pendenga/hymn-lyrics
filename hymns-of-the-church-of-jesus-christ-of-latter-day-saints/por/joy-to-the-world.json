{
  "number": 121,
  "title": "Mundo Feliz, Nasceu Jesus",
  "marking": "Jubiloso",
  "verses": [
    {
      "number": 1,
      "lines": [
        "Mundo feliz, nasceu Jesus.",
        "Nasceu trazendo a luz!",
        "Trazendo a salva\u00e7\u00e3o,",
        "Trazendo a reden\u00e7\u00e3o,",
        "Louvemos ao Senhor,",
        "Louvemos ao Senhor!",
        "Louvemos, louvemos ao Senhor!"
      ]
    },
    {
      "number": 2,
      "lines": [
        "Mundo feliz, o seu perd\u00e3o",
        "Roguemos em uni\u00e3o!",
        "Na terra e no mar,",
        "Cantemos sem cessar,",
        "Cantemos seu louvor,",
        "Cantemos seu louvor!",
        "Cantemos, cantemos seu louvor!"
      ]
    },
    {
      "number": 3,
      "lines": [
        "Mundo feliz, hoje afinal,",
        "Vencido foi o mal!",
        "Em nosso cora\u00e7\u00e3o,",
        "Em santa contri\u00e7\u00e3o,",
        "Roguemos seu perd\u00e3o,",
        "Roguemos seu perd\u00e3o!",
        "Roguemos, roguemos seu perd\u00e3o!"
      ]
    },
    {
      "number": 4,
      "lines": [
        "Mundo feliz, louvai a Deus",
        "Aos gratos dotes seus!",
        "O Filho nos mandou,",
        "Ao pecador salvou",
        "Hosanas ao Senhor,",
        "Hosanas ao Senhor!",
        "Hosanas, hosanas ao Senhor!"
      ]
    }
  ],
