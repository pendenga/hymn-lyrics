{
  "number": 162,
  "title": "Com Valor Marchemos",
  "marking": "Marcial",
  "verses": [
    {
      "number": 1,
      "lines": [
        "Com valor marchemos,",
        "Hostes de Jesus,",
        "Empunhando as armas",
        "Da divina luz.",
        "Nosso comandante",
        "O Senhor ser\u00e1;",
        "Seu pend\u00e3o no alto,",
        "Se desfralda j\u00e1.",
        "Com valor marchemos,",
        "Hostes de Jesus,",
        "Empunhando as armas",
        "Da divina luz!"
      ]
    },
    {
      "number": 2,
      "lines": [
        "Ao soar a carga",
        "Foge Satan\u00e1s;",
        "Firmes, pois, marchemos,",
        "N\u00e3o tardemos mais.",
        "O inferno treme",
        "Cheio de pavor,",
        "Ao Senhor cantemos",
        "Hinos de louvor.",
        "Com valor marchemos,",
        "Hostes de Jesus,",
        "Empunhando as armas",
        "Da divina luz!"
      ]
    },
    {
      "number": 3,
      "lines": [
        "Que progrida sempre",
        "O labor de Deus;",
        "Juntos, pois, marchemos,",
        "Qual soldados seus.",
        "Juntos, sempre juntos,",
        "Sempre em uni\u00e3o",
        "E na esperan\u00e7a",
        "Do final perd\u00e3o.",
        "Com valor marchemos,",
        "Hostes de Jesus,",
        "Empunhando as armas",
        "Da divina luz!"
      ]
    },
    {
      "number": 4,
      "lines": [
        "Vamos, camaradas,",
        "Vamos sem temor",
        "E cantemos juntos,",
        "Cheios de valor.",
        "Nosso Pai Eterno",
        "Todos honrar\u00e3o;",
        "Anjos com os homens",
        "Juntos cantar\u00e3o.",
        "Com valor marchemos,",
        "Hostes de Jesus,",
        "Empunhando as armas",
        "Da divina luz!"
      ]
    }
  ],
  "credits": [
