{
  "number": 89,
  "title": "Ao Partir Cantemos",
  "marking": "Reverente",
  "verses": [
    {
      "number": 1,
      "lines": [
        "Ao partir cantemos",
        "Hinos de louvor",
        "Ao Divino Mestre,",
        "Por seu grande amor",
        "Por seus gratos dotes",
        "E cuidados seus",
        "Subam nossos hinos",
        "Sempre, sempre a Deus."
      ]
    },
    {
      "number": 2,
      "lines": [
        "Pela esperan\u00e7a",
        "No final perd\u00e3o,",
        "Juntos entoamos",
        "Nossa gratid\u00e3o",
        "C\u00e2nticos alegres,",
        "Cheios de fervor,",
        "Ao Senhor elevam",
        "Nosso grande amor."
      ]
    },
    {
      "number": 3,
      "lines": [
        "Hoje aqui cantemos",
        "Hinos ao Senhor,",
        "A Jesus trazendo",
        "Nossa f\u00e9 e ardor.",
        "Salva-nos \u00f3 Cristo",
        "Para te servir;",
        "D\u00e1-nos teu aux\u00edlio",
        "Para te seguir."
      ]
    }
  ],
