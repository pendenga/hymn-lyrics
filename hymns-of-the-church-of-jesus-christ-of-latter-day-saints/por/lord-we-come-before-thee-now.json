{
  "number": 95,
  "title": "Eis-nos, Hoje, a Teus P\u00e9s",
  "marking": "Reverente",
  "verses": [
    {
      "number": 1,
      "lines": [
        "Eis-nos hoje a teus p\u00e9s,",
        "Dadivoso \u00f3 Deus tu \u00e9s!",
        "Imploramos prote\u00e7\u00e3o.",
        "Buscaremos-te em v\u00e3o?"
      ]
    },
    {
      "number": 2,
      "lines": [
        "Reunidos hoje aqui,",
        "A buscar a paz em ti,",
        "N\u00e3o nos deixes dispersar",
        "Sem tua b\u00ean\u00e7\u00e3o desfrutar."
      ]
    },
    {
      "number": 3,
      "lines": [
        "Na mensagem de amor",
        "Da palavra do Senhor",
        "Vem aos tristes consolar,",
        "Sua dor aliviar."
      ]
    },
    {
      "number": 4,
      "lines": [
        "Aos enfermos vem curar,",
        "Aos cativos libertar,",
        "S\u00ea o nosso protetor,",
        "Nosso Pai e Deus de amor."
      ]
    }
  ],
