{
  "number": 122,
  "title": "Erguei-vos Cantando",
  "marking": "Majestoso",
  "verses": [
    {
      "number": 1,
      "lines": [
        "Erguei-vos cantando,",
        "Cheios de triunfo,",
        "E vinde, \u00f3 vinde todos para Bel\u00e9m;",
        "Hoje \u00e9 nascido",
        "O Senhor dos anjos.",
        "\u00d3 vinde, adoremos;",
        "\u00d3 vinde, adoremos;",
        "\u00d3 vinde, adoremos a Cristo Jesus."
      ]
    },
    {
      "number": 2,
      "lines": [
        "Cantai-lhe louvores,",
        "Em celestes coros,",
        "Ressoe na terra o grande clamor.",
        "Gl\u00f3ria cantemos",
        "Ao Deus Eterno.",
        "\u00d3 vinde, adoremos;",
        "\u00d3 vinde, adoremos;",
        "\u00d3 vinde, adoremos a Cristo Jesus."
      ]
    },
    {
      "number": 3,
      "lines": [
        "Senhor, jubilosos,",
        "Teu natal cantamos;",
        "\u00d3 Cristo a ti ser\u00e1 a gl\u00f3ria sem fim.",
        "Verbo divino,",
        "De Deus ungido.",
        "\u00d3 vinde, adoremos;",
        "\u00d3 vinde, adoremos;",
        "\u00d3 vinde, adoremos a Cristo Jesus."
      ]
    }
  ],
