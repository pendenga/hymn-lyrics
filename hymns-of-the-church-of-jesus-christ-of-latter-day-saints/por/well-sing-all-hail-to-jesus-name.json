{
  "number": 105,
  "title": "Cantemos Todos a Jesus",
  "marking": "Com fervor",
  "verses": [
    {
      "number": 1,
      "lines": [
        "Cantemos todos a Jesus,",
        "Hosanas e louvor;",
        "A ele que na cruz morreu,",
        "Cantemos com fervor."
      ]
    },
    {
      "number": 2,
      "lines": [
        "Mil sofrimentos suportou,",
        "Por nossa salva\u00e7\u00e3o;",
        "Ao homem chama com amor",
        "E d\u00e1-lhe prote\u00e7\u00e3o."
      ]
    },
    {
      "number": 3,
      "lines": [
        "Venceu a tumba e o mal,",
        "O tentador feriu;",
        "Os pecadores libertou;",
        "Da morte ressurgiu."
      ]
    },
    {
      "number": 4,
      "lines": [
        "Tomai do c\u00e1lice e do p\u00e3o,",
        "Em santa comunh\u00e3o;",
        "Tomai, \u00f3 santos, sim, mostrai",
        "A f\u00e9 no cora\u00e7\u00e3o."
      ]
    }
  ],
