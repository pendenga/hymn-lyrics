{
  "number": 181,
  "title": "Jesus of Nazareth Savior and King",
  "marking": "Worshipfully",
  "verses": [
    {
      "number": 1,
      "lines": [
        "Jesus of Nazareth,",
        "Savior and King!",
        "Triumphant over death,",
        "Life thou didst bring,",
        "Leaving thy Father\u2019s throne,",
        "On earth to live,",
        "Thy work to do alone,",
        "Thy life to give."
      ]
    },
    {
      "number": 2,
      "lines": [
        "While of this broken bread",
        "Humbly we eat,",
        "Our thoughts to thee are led",
        "In rev\u2019rence sweet.",
        "Bruised, broken, torn for us",
        "On Calvary\u2019s hill\u2014",
        "Thy suff\u2019ring borne for us",
        "Lives with us still."
      ]
    },
    {
      "number": 3,
      "lines": [
        "As to our lips the cup",
        "Gently we press,",
        "Our hearts are lifted up;",
        "Thy name we bless!",
        "Guide us where\u2019er we go,",
        "Till in the end",
        "Life evermore we\u2019ll know",
        "Through thee, our Friend."
      ]
    }
  ],
  "credits": [
    "Text and music: Hugh W. Dougall, 1872\u20131963"
  ],
