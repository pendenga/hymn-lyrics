{
  "number": 37,
  "title": "Glorias cantad a Dios",
  "marking": "Con fervor",
  "verses": [
    {
      "number": 1,
      "lines": [
        "Glorias cantad a Dios",
        "con poderosa voz.",
        "Glorias cantad.",
        "Por compasi\u00f3n obr\u00f3, mil penas aguant\u00f3",
        "y en la cruz muri\u00f3 el Redentor."
      ]
    },
    {
      "number": 2,
      "lines": [
        "Cristo, el Salvador,",
        "libr\u00f3 al pecador.",
        "Glorias cantad.",
        "Su triunfo proclamad; de muerte libertad nos dio",
        "por Su bondad el Redentor."
      ]
    },
    {
      "number": 3,
      "lines": [
        "Suene el son triunfal",
        "de hueste celestial.",
        "Glorias cantad.",
        "Grande Tu dignidad, honra y majestad",
        "por la eternidad, oh Redentor."
      ]
    }
  ],
